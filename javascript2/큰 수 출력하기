<html>
    <head>
        <meta charset="UTF-8">
        <title>출력결과</title>
    </head>
    <body>
        <script>
            function solution(arr){
                let answer = [];
                let N = arr.length;
                let nIndex = arr.findIndex(item => item === N);
                answer = arr.filter(item => item > arr[nIndex-1])
                return answer;
            }
            let arr = [7,3,9,5,6,12];
            console.log(solution(arr))
        </script>
    </body>
</html>
