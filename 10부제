        <script>
            function solution(day, arr){
                let answer=0;
                const test = Array.from(String(25), Number);
                answer = arr.filter(item => day === Array.from(String(item), Number)[1]).length
                return answer;
            }
            
            arr=[25, 23, 11, 47, 53, 17, 33];
            console.log(solution(3, arr));
        </script>
